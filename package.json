{
  "name": "sfdx-generator",
  "version": "5.0.0",
  "description": "A node wrapper for Salesforce DX",
  "keywords": [
    "sfdx",
    "wrapper",
    "salesforce",
    "generator",
    "dx"
  ],
  "main": "./generator/dist/sfdx-generator.umd.js",
  "module": "./generator/dist/sfdx-generator.cjs.js",
  "typings": "./generator/dist/types/sfdx-generator.d.ts",
  "files": [
    "generator/dist"
  ],
  "publishConfig": {
    "init-author-email": "sandbox_JSUI@coveo.com",
    "init.author.name": "Coveo",
    "init.author.url": "http://source.coveo.com/"
  },
  "author": "Marc-Antoine Veilleux <maveilleux@coveo.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/coveo/sfdx-generator.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint -t codeFrame 'generator/**/*.ts'",
    "build": "rimraf generator/dist && rimraf src/Client/Commands/Generated/* && tsc",
    "run-generator": "node generator/tools/generate-files.js",
    "generate": "npm run build && sfdx force:doc:commands:display --json > generator/commands.json && npm run run-generator"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "fs-extra": "^7.0.0",
    "reflect-metadata": "^0.1.12",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^7.0.0",
    "@types/node": "^10.7.1",
    "@types/underscore": "^1.8.9",
    "colors": "^1.3.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "lint-staged": "^7.2.2",
    "lodash.camelcase": "^4.3.0",
    "moment": "^2.22.2",
    "prettier": "^1.14.2",
    "sfdx-cli": "7.43.1",
    "ts-node": "^7.0.1",
    "tsc-watch": "^1.0.26",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.0.1"
  }
}
